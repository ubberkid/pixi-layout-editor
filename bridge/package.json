{
  "name": "pixi-layout-editor",
  "version": "0.1.0",
  "description": "Debug bridge for PixiJS layout editor - inspect and edit @pixi/layout properties in real-time",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "import": "./dist/vite-plugin.js",
      "types": "./dist/vite-plugin.d.ts"
    }
  },
  "files": [
    "dist",
    "editor-dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "pixi",
    "pixijs",
    "pixi-layout",
    "layout",
    "editor",
    "debug",
    "flexbox",
    "vite"
  ],
  "author": "ubberkid",
  "license": "MIT",
  "peerDependencies": {
    "pixi.js": "^8.0.0",
    "vite": "^5.0.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "pixi.js": "^8.6.6",
    "typescript": "^5.6.3",
    "vite": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ubberkid/pixi-layout-editor"
  },
  "homepage": "https://github.com/ubberkid/pixi-layout-editor#readme"
}
